<?xml version="1.0" encoding="utf-8"?>
<resources>
<!-- Theme applied to the Android window when the operating system's dark mode setting is turned off -->
    <style name="LaunchTheme" parent="@android:style/Theme.Light.NoTitleBar">
<!-- Display a splash screen on the activity. Automatically removed when the Flutter engine renders the first frame -->
        <item name="android:windowBackground">@drawable/launch_background</item>
    </style>
<!-- Theme applied to the Android window immediately after process launch.
         This theme determines the color of the Android window during Flutter UI initialization and the color behind the Flutter UI while it's running.

         This theme is only used in Flutter Android embedding V2. -->
    <style name="NormalTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowBackground">?android:colorBackground</item>
    </style>
</resources>
